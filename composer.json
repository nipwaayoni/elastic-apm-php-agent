{
    "name" : "nipwaayoni/elastic-apm-php-agent",
    "description": "A php agent for Elastic APM v2 Intake API",
    "license": "MIT",
    "require" : {
        "php" : ">= 7.1",
        "guzzlehttp/guzzle": "6.*",
        "ext-curl" : "*",
        "ext-json": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^9.1"
    },
    "autoload" : {
        "psr-4" : {
            "Nipwaayoni\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nipwaayoni\\Tests\\": "tests/"
        }
    },
    "config" : {
        "optimize-autoloader" : true
    },
    "authors" : [
        {
